.hight_max{
  height: 100%;
  padding-bottom: 500px;
}

.about_image{
  background-image: image-url("about.jpg");
  background-position: center;
  background-size: cover;
  background-repeat: no-repeat;
  background-attachment: fixed;
}

.pagination>li>a {
  border: none;
  color: #696969;
}

.pagination>.active>a {
  background: #93c9ff;
  border-radius: 15px;
}

.pagination>li>a:hover {
  border-radius: 15px;
}


@media (max-width: 600px) {
  .sidebar {
    display: none;
  }
}